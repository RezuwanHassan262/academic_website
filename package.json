{
  "name": "homepage-typescript",
  "version": "0.8.1.1",
  "description": "personal website - TypeScript version",
  "repository": {
    "type": "git",
    "url": "https://github.com/asib27/asib27.github.io"
  },
  "keywords": [
    "jekyll",
    "theme",
    "minimal",
    "typescript"
  ],
  "contributors": [
    "Asib Rahman",
    "Forked from Academic Homepage"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/academicpages/asib27.github.io/issues"
  },
  "homepage": "https://github.com/academicpages/asib27.github.io",
  "engines": {
    "node": ">= 18.0.0"
  },
  "dependencies": {
    "fitvids": "^2.1.1",
    "jquery": "^3.7.1",
    "jquery-smooth-scroll": "^2.2.0",
    "magnific-popup": "^1.2.0",
    "pandas-js": "^0.2.4"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.30",
    "@types/node": "^20.0.0",
    "concurrently": "^9.2.1",
    "http-server": "^14.1.1",
    "onchange": "^7.1.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.9.3",
    "uglify-js": "^3.17.4",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "http-server . -p 8080 -o",
    "dev": "concurrently \"npm run watch\" \"npm run serve\"",
    "build": "npm run build:ts && npm run build:js",
    "build:ts": "tsc",
    "build:js": "webpack --mode production",
    "watch": "tsc --watch",
    "uglify": "uglifyjs node_modules/jquery/dist/jquery.min.js node_modules/fitvids/dist/fitvids.min.js node_modules/magnific-popup/dist/jquery.magnific-popup.min.js node_modules/jquery-smooth-scroll/jquery.smooth-scroll.min.js dist/assets/js/plugins/jquery.greedy-navigation.js dist/assets/js/_main.js -c -m -o assets/js/main.min.js",
    "watch:js": "onchange \"src/**/*.ts\" \"assets/js/**/*.ts\" -e \"assets/js/main.min.js\" -- npm run build:ts && npm run build:js",
    "install:python": "pip install pandas numpy requests beautifulsoup4 getorg geopy typing-extensions",
    "convert:python": "python scripts/convert_python_to_ts.py"
  }
}
